         jLabel3.setVisible(false);
    }                                        

    private void discounttxtFocusGained(java.awt.event.FocusEvent evt) {                                        
        // TODO add your handling code here:
          if(discounttxt.getText().equals("example : 0.5 "))
          {
              discounttxt.setText("");
              discounttxt.setForeground(new Color(153,153,153));  
          }
    }                                       

