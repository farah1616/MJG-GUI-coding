    private void txtpaidKeyPressed(java.awt.event.KeyEvent evt) {                                   
        // TODO add your handling code here:
        
        
        if(evt.getKeyCode()==KeyEvent.VK_ENTER){
        double balance = 0.00;
        balance =(Double.valueOf(txtpaid.getText()))-(Double.valueOf(totalprice.getText()));
        //balance = (Double.valueOf(totalprice.getText()))- paid;
        txtbalance.setText(Double.toString(balance));
        }
    }                                  

    private void discounttxtKeyPressed(java.awt.event.KeyEvent evt) {                                       
        // TODO add your handling code here:
        //try test
    }                                      

    private void calculatebutton1ActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        // TODO add your handling code here:
   
         int numrow=jTablecheckout.getRowCount();
        double total=0.00;
        double netprice =0.00;
        //totalprice=new JTextField();
       //DecimalFormat dc= new DecimalFormat("0.00");
       // String total_Price=String.format("%.2f",netprice);
        //String formattedText=dc.format(Double.parseDouble(totalprice.getText()));
        
        
        for(int i=0;i<numrow;i++){
            //get from qty column
            int qty =Integer.valueOf(jTablecheckout.getValueAt(i,2).toString());
            //get from price column
            double value = Double.valueOf(jTablecheckout.getValueAt(i,1).toString())*qty;
        
            total+=value;
        }
        netprice=(1.00-(Double.valueOf(discounttxt.getText())))*total;
        totalprice.setText(Double.toString(netprice));
        
        
    }                                                

