    private void receiptbuttonActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
        String paymentType;
        
        if(jComboBox1.getSelectedItem().equals("Debit/Credit Card")){
            paymentType = "Debit/Credit Card";
        }
        else
            paymentType = "Cash";
        
       try{
            FileWriter file = new FileWriter("Receipt.txt",true);
            BufferedWriter bw =new BufferedWriter(file);
            //PrintWriter out = new PrintWriter(file);

            
            bw.write("\n====================================Receipt"
                    + "=====================================\n");
            bw.write("\nReceipt No : "+receiptnum.getText());
            bw.write("\n\nCode\t\t\t\tPrice\t\t\t\tQuantity");
            bw.write("\n_____________________________________________"
                    + "____________________________________\n");
            for(int i=0; i<jTablecheckout.getRowCount();i++){//row
                bw.newLine();
                for(int j=0; j<jTablecheckout.getColumnCount();j++){//columns
                    bw.write(jTablecheckout.getValueAt(i,j).toString()+"\t\t\t\t"); 
                    
                }

            }
            bw.newLine();
            bw.write("\n----------------------------------------------------"
                    + "-----------------------------\n");
            bw.write("\nPayment Type: "+paymentType);
            bw.write("\nTotal Price: RM "+totalprice.getText());
            bw.write("\nDiscount:  "+discounttxt.getText()+" percent");
            bw.write("\nPaid: RM "+txtpaid.getText());
            bw.write("\nBalance: RM "+txtbalance.getText());
            bw.write("\n=================================================="
                    + "================================\n");
            bw.newLine();   
            bw.close();
            file.close();
            
            
        }catch(IOException ex){
          Logger.getLogger(customerpage.class.getName()).log(Level.SEVERE, null, ex);  
        }
        JOptionPane.showMessageDialog(null,"Receipt stored!");

    }                                             

    private void enterbuttonActionPerformed(java.awt.event.ActionEvent evt) {                                            
         // TODO add your handling code here:
        //get table model
        DefaultTableModel tblModel=(DefaultTableModel)jTablecheckout.getModel();
        if(jTablecheckout.getSelectedRowCount()== 1){
            //if single row is selected than update
            
 
            String qty = quantity.getText();
        
            
            //set updated value

            tblModel.setValueAt(qty,jTablecheckout.getSelectedRow(),2);
 
        }else{
            if(jTablecheckout.getRowCount()==0){
                JOptionPane.showMessageDialog(null,"No Quantity","Error",JOptionPane.ERROR_MESSAGE);
            }else{
                //if row is not selected or selected multiple
                JOptionPane.showMessageDialog(null,"Please Select Single Row for enter Quantity!");
                
            }
        }
 
    }                                           

    private void deltebuttomActionPerformed(java.awt.event.ActionEvent evt) {                                            
         // TODO add your handling code here:
        DefaultTableModel tblModel=(DefaultTableModel) jTablecheckout.getModel();
        
        //delete row
        if(jTablecheckout.getSelectedRowCount()==1){
            //if single row selected so delete
            tblModel.removeRow(jTablecheckout.getSelectedRow());
        }else{
            if(jTablecheckout.getRowCount()==0){
                //if table is empty(no data)than display message
                JOptionPane.showMessageDialog(this,"No item to be deleted!");
                }else{
                //if tablle not empty but row is not selected or multiple selected
                JOptionPane.showMessageDialog(this,"Please Select Single Row for Delete!");
            }
        }
    }                                           

    private void btncheckoutActionPerformed(java.awt.event.ActionEvent evt) {                                            
         // TODO add your handling code here:                                         
        // TODO add your handling code here:
        TableModel model1 =jTableAvailableItems.getModel();
        int[]indexs =jTableAvailableItems.getSelectedRows();//table available items
        Object[]row=new Object[3];
        DefaultTableModel model2 =(DefaultTableModel) jTablecheckout.getModel();
        for(int i =0; i<indexs.length;i++)
        {
           // if(getSelectedRow(indexs[i],4)!=0)
            row[0]=model1.getValueAt(indexs[i],0);
            row[1]=model1.getValueAt(indexs[i],3);
            
            model2.addRow(row);      
        }  
    }                                           

    private void btnReturnActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        JOptionPane.showMessageDialog(null,"Return to main menu!");
        new MenuList().setVisible(true);
        setVisible(false);
    }                                         

    private void btnImportData1ActionPerformed(java.awt.event.ActionEvent evt) {                                               
        // TODO add your handling code here:
         
        //import data from txt
        String code,name,brand,inCode,inName,inBrand;
        double price,inPrice;
        int stock,inStocks;
        
        boolean found=false;
        
        String fileName = "items.txt";
        File file = new File(fileName);
 
        try {
            FileReader reader= new FileReader(file); 
            BufferedReader br = new BufferedReader(reader);
            
            DefaultTableModel model = (DefaultTableModel)jTableAvailableItems.getModel();
            Object[] lines = br.lines().toArray();
            for(int i = 0; i <lines.length; i++){
                String[] row = lines[i].toString().split(";");
                model.addRow(row);
                
            }
            reader.close();
            br.close(); 
            
             } catch (FileNotFoundException ex) {
            Logger.getLogger(Userpage.class.getName()).log(Level.SEVERE, null, ex);
            
        }  catch (IOException ex) {
            Logger.getLogger(Userpage.class.getName()).log(Level.SEVERE, null, ex);
        }
                                                 
    }                                              

